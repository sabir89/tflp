<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.7.10">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Inquiry]]></title>
		<description><![CDATA[This form is used for modal pop up inquiry box]]></description>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Submit]]></text>
		</button>
		<fields>
			<field allowsPrepopulate="1" id="3" size="medium" type="text">
				<cssClass><![CDATA[mlsnumber_inquiry]]></cssClass>
				<inputName><![CDATA[mlnumber_inquiry]]></inputName>
				<label><![CDATA[Listing MLS#]]></label>
			</field>
			<field allowsPrepopulate="1" id="4" size="medium" type="hidden">
				<inputName><![CDATA[mlnumber]]></inputName>
				<label><![CDATA[MLS#]]></label>
			</field>
			<field id="5" size="medium" type="section" displayOnly="1">
				<label><![CDATA[Property Listing Inquiry]]></label>
			</field>
			<field id="5" size="medium" type="section" displayOnly="1">
				<label><![CDATA[Property Listing Inquiry]]></label>
			</field>
			<field id="1" isRequired="1" size="medium" type="name">
				<label><![CDATA[Name]]></label>
				<inputs>
					<input id="1.3">
						<label><![CDATA[First]]></label>
					</input>
					<input id="1.6">
						<label><![CDATA[Last]]></label>
					</input>
				</inputs>
			</field>
			<field id="2" isRequired="1" size="medium" type="email">
				<label><![CDATA[Email]]></label>
			</field>
			<field id="6" isRequired="1" size="medium" type="phone" phoneFormat="standard">
				<label><![CDATA[Phone]]></label>
			</field>
			<field id="7" size="medium" type="textarea">
				<label><![CDATA[Message]]></label>
			</field>
			<field id="8" size="medium" type="radio">
				<label><![CDATA[Contact Me]]></label>
				<choices>
					<choice>
						<text><![CDATA[By Email]]></text>
					</choice>
					<choice>
						<text><![CDATA[By Phone]]></text>
					</choice>
				</choices>
			</field>
		</fields>
		<confirmations>
			<confirmation id="51d5a9994cdf3" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
		<notifications>
			<notification id="51d5a9994cb77">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Pristine Notification]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
				<from><![CDATA[{admin_email}]]></from>
				<fromName><![CDATA[{embed_url}{date_mdy}{ip}]]></fromName>
				<replyTo><![CDATA[{Email:2}]]></replyTo>
			</notification>
		</notifications>
	</form>
</forms>